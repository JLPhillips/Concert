/#map-canvas
#top
  #overall
    .row#optionsbox
      .small-4.columns
        %select#options{type: "text"}
          %option Select Section
          %option{value: "ga"} General Admission
          %option{value: "vip"} VIP
      .small-4.columns
        %input#numSeats{type: "text", placeholder: "Number of Seats"}
      .small-4.columns
        %input#create.button.radius.small.success.expand{type: "button", value: "Create"}
#bottom
  #overall2
    #stage
      %h1 STAGE
    #vip
    #ga
  #namecontainer
    .row
      .small-2.columns
        %input#name{type: "text", placeholder: "Enter Name"}
      .small-10.columns
  #reportingLeftContainer
    #reportingLeft
  #reportingRightContainer
    #reportingRight
      %h5 Reserved Seats
      %table
        %tr
          %th
          %th Seat
          %th NAME
        %tr
        %tr